{
  "name": "coin98-vault-js",
  "version": "1.0.0",
  "description": "---",
  "main": "services/index.js",
  "scripts": {
    "maintain": "anchor test --skip-local-validator",
    "test": "anchor test --skip-build --skip-deploy --skip-local-validator",
    "build:services": "tsc",
    "build:program": "anchor build",
    "build": "npm run build:program && npm run build:services",
    "prerelease": "npm run test && npm run build && cp .npmrc dist",
    "release": "standard-version && cp package.json dist && git push --follow-tags origin main && cd dist && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coin98/coin98-vault.git"
  },
  "author": "Coin98",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coin98/coin98-vault/issues"
  },
  "homepage": "https://github.com/coin98/coin98-vault#readme",
  "dependencies": {
    "@coin98/solana-support-library": "0.2.0",
    "@project-serum/anchor": "^0.24.2",
    "@project-serum/borsh": "^0.2.5",
    "@types/chai": "^4.3.1",
    "bn.js": "^5.2.1",
    "moment": "^2.29.3",
    "standard-version": "^9.5.0"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/mocha": "^9.0.0",
    "chai": "^4.3.6",
    "mocha": "^9.0.3",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.3.5"
  }
}
